extends layout

block content
    each c in checkers
        .row.container
            .col.s8
                .card.blue-grey.darken-1
                    .card-content.white-text
                        span.card-title= e.title
                        p= e.url
                    .card-action
                        a(href="#") 报名
                        a(href="#") 详情

    .container
        ul.pagination
            li.disabled
                a(href="#!")
                    i.material-icons chevron_left
            li.active.teal.darken-3
                a(href="#!") 1
            li.waves-effect
                a(href="#!") 2
            li.waves-effect
                a(href="#!") 3
            li.waves-effect
                a(href="#!")
                    i.material-icons chevron_right