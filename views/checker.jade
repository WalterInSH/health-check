extends layout

block content
    h3= checker.title
    div= checker.url

    table.highlight
        thead
            tr
                th 时间
                th HTTP 状态码
        tbody
        each log in logs
            tr
                td= moment(log.create_date).format('YYYY MM DD hh:mm:ss')
                td= log.status_code

    .container
        ul.pagination
            li.disabled
                a(href="#!")
                    i.material-icons chevron_left
            li.active.teal.darken-3
                a(href="#!") 1
            li.waves-effect
                a(href="#!") 2
            li.waves-effect
                a(href="#!") 3
            li.waves-effect
                a(href="#!")
                    i.material-icons chevron_right

